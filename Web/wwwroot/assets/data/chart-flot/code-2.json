&lt;div id="interactive-chart" class="h-250px"&gt;&lt;/div&gt;

&lt;script&gt;
  var d1 = [[0, 42], [1, 53], [2,66], [3, 60], [4, 68], [5, 66], [6,71],[7, 75], [8, 69], [9,70], [10, 68], [11, 72], [12, 78], [13, 86]];
