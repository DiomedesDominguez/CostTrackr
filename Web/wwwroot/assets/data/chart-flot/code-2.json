  $.plot($('#interactive-chart'), [{
    data: d1, 
    label: 'Page Views', 
    color: app.color.blue,
    lines: { show: true, fill:false, lineWidth: 2.5 },
    points: { show: true, radius: 5, fillColor: app.color.componentBg },
