      clickable: true,
      tickColor: 'rgba('+ app.color.darkRgb + ', .15)',
      borderWidth: 1,
      borderColor: 'rgba('+ app.color.darkRgb + ', .15)',
      backgroundColor: 'rgba('+ app.color.darkRgb + ', .035)'
